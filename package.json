{
  "name": "payout-service",
  "version": "1.0.0",
  "description": "Payout service",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./bin/www",
    "start": "node ./bin/www",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "pretest": "cross-env NODE_ENV=test  npm run db:migrate:test && npm run seed:test",
    "seed:test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
    "db:migrate:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all",
    "db:seed:dev": "cross-env NODE_ENV=development npx sequelize-cli db:seed:all"
  },
  "keywords": [
    "payment"
  ],
  "author": "Olajuwon lawal",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hapi/joi": "^15.0.1",
    "@sendgrid/mail": "^8.1.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "debug": "^4.3.4",
    "express": "^4.19.1",
    "express-async-errors": "^3.1.1",
    "faker": "^6.6.6",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "jade": "^1.11.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "logger": "^0.0.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.25",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  }
}
